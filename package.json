{
  "name": "analytics-dashboard-tailadmin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest --config vitest.config.mjs",
    "test:ui": "vitest --ui --config vitest.config.mjs",
    "test:run": "vitest run --config vitest.config.mjs",
    "test:coverage": "vitest run --coverage --config vitest.config.mjs",
    "test:watch": "vitest --watch --config vitest.config.mjs",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:run && npm run test:e2e",
    "validate": "npm run validate:types && npm run validate:tests && npm run validate:lint",
    "validate:types": "tsc --noEmit",
    "validate:tests": "npm run test:run",
    "validate:lint": "npm run lint",
    "validate:build": "npm run db:ensure && npm run build",
    "pre-push": "npm run db:ensure && npm run validate && npm run validate:build",
    "prepare": "npm run postinstall",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:ensure": "node scripts/ensure-prisma.js",
    "fix:prisma": "powershell -ExecutionPolicy Bypass -File fix-prisma-windows.ps1"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.9.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.14.0",
    "@tanstack/react-query": "^5.85.5",
    "@trpc/client": "^11.4.4",
    "@trpc/react-query": "^11.4.4",
    "@trpc/server": "^11.4.4",
    "apexcharts": "^5.3.2",
    "d3-geo": "^3.1.1",
    "flatpickr": "^4.6.13",
    "googleapis": "^155.0.0",
    "lucide-react": "^0.539.0",
    "motion": "^12.23.21",
    "next": "15.4.5",
    "next-intl": "^4.3.4",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-apexcharts": "^1.7.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "19.1.0",
    "superjson": "^2.2.2",
    "topojson-client": "^3.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3-geo": "^3.1.0",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/topojson-client": "^3.1.5",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "happy-dom": "^20.0.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.5",
    "playwright": "^1.56.0",
    "prisma": "^6.14.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vitest-fetch-mock": "^0.4.5"
  }
}
